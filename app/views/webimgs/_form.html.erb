<%= form_for(@webimg) do |f| %>
  <% if @webimg.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@webimg.errors.count, "error") %> prohibited this webimg from being saved:</h2>

      <ul>
	  <!-- Paperclip validation errors-->
      <!-- and custom validation errors show up here -->
      <% @webimg.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.text_field :name ,class:'form-control',placeholder:' Please provide Image title' %>
  </div>
  <!-- Upload your avatar via a remote url in a form_tag -->
  <div class="field">
	  <%= text_field_tag 'avatar_url', nil,class:'form-control',placeholder:' Please provide Image URL'  %>
  </div>
  <div class="actions">
    <%= f.submit :Upload, class: 'btn btn-default' %>
  </div>
<% end %>